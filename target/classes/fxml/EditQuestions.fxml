<AnchorPane id="AnchorPane" minHeight="570.0" minWidth="750.0" prefHeight="570.0" prefWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="tn.esprit.overpowered.pijavafx.controllers.EditQuestionsController">
   <children>
      <HBox layoutX="242.0" layoutY="509.0" prefHeight="37.0" prefWidth="266.0" spacing="20.0">
