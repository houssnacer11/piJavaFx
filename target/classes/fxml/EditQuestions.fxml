          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <JFXButton fx:id="addQuestionBtn" layoutX="310.0" layoutY="352.0" onAction="#onAddQuestionBtnClicked" style="-fx-background-color: white;" text="Add question" textFill="#6b5e5e">
         <font>
            <Font size="18.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
